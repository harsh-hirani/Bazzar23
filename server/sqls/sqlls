-- CREATE TABLE `bazzar`.`stocklinear_hdfc` ( `id` INT(255) NOT NULL AUTO_INCREMENT , `datetime` BIGINT(20) NOT NULL , `buyOrSell` INT(2) NOT NULL , `quantity` INT(50) NOT NULL , `newPrice` INT(50) NOT NULL , `beforePrice` INT(50) NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB;

CREATE TABLE `stocklinear_axis` ( `id` INT(255) NOT NULL AUTO_INCREMENT , `datetime` BIGINT(20) NOT NULL , `buyOrSell` INT(2) NOT NULL COMMENT 'buy = 0\r\nsell = 1' , `quantity` INT(200) NOT NULL , `newPrice` INT(50) NOT NULL , `beforePrice` INT(50) NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB;

CREATE TABLE `stock_profile_tata` ( `id` INT(255) NOT NULL AUTO_INCREMENT , `time` BIGINT(20) NOT NULL , `open` INT(11) NOT NULL , `high` INT(11) NOT NULL , `low` INT(11) NOT NULL , `close` INT(11) NOT NULL , `volume` INT(11) NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB;


